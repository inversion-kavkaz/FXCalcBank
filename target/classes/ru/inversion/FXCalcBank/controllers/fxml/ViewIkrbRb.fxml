                <JInvTableColumn fx:id="CUSRLOGNAME" fieldName="CUSRLOGNAME" text="%CUSRLOGNAME" />
                <JInvTableColumn fx:id="CBIC" fieldName="CBIC" text="%CBIC" />
            </columns>
        </JInvTable>
       <VBox spacing="5.0" >
           <children>
               <JInvToolBar fx:id="toolBar" />
               <JInvTable fx:id="IKRB_TYPEEV" tableMenuButtonVisible="true" VBox.vgrow="ALWAYS">
