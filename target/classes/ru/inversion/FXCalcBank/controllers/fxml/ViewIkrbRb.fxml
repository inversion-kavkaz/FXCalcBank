                       <JInvTableColumn fx:id="ITYPEEV_ACCC" text="%ITYPEEV_ACCC" fieldName="ITYPEEV_ACCC"  />
                   </columns>
                   <VBox.margin>
                       <Insets bottom="5.0" left="5.0" right="5.0" />
                   </VBox.margin>
               </JInvTable>
           </children>
       </VBox>   </items>
</SplitPane>
