<?import javafx.scene.layout.VBox?>
<?import ru.inversion.fx.form.controls.JInvToolBar?>
<?import javafx.geometry.Insets?>
<SplitPane dividerPositions="0.5" orientation="VERTICAL" xmlns="http://javafx.com/javafx/8.0.202" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="ru.inversion.FXCalcBank.controllers.ViewIkrbRbController">
   <items>
        <JInvTable fx:id="IKRB_RB" tableMenuButtonVisible="true">
            <columns>
                <JInvTableColumn fx:id="IRBNUM" fieldName="IRBNUM" text="%IRBNUM" />
