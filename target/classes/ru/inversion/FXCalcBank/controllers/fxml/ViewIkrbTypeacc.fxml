                <JInvTableColumn fx:id="ITYPE_ACC" text="%ITYPE_ACC" fieldName="ITYPE_ACC"/>
                <JInvTableColumn fx:id="CTYPE_ACCNAME" text="%CTYPE_ACCNAME" fieldName="CTYPE_ACCNAME"
